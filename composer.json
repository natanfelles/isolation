{
	"name": "natanfelles/isolation",
	"type": "library",
	"description": "The Framework Isolation Library",
	"keywords": [
		"isolated",
		"isolation",
		"require"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/isolation.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ergebnis/composer-normalize": "^2.15",
		"natanfelles/coding-standard": "^1.6",
		"phpmd/phpmd": "^2.10",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.5"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"files": [
			"src/isolation.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Isolation\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/isolation/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/isolation/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/isolation",
		"docs": "https://the-framework.gitlab.io/libraries/isolation/docs/"
	}
}
